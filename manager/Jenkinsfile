pipeline {
    agent any

    stages {
        stage ('Directory Stage') {
                steps {
                        bat 'cd manager'
                }
            }
        stage ('Compile Stage') {
            steps {
                    bat 'mvn clean compile'
            }
        }
        stage ('Deployment Stage') {
            steps {

                    bat 'mvn deploy'

            }
        }
    }
}